private class User {
    private String Modelo;
    private String Marca;

    public User(String Modelo, String Marca) {
        this.Modelo = modole;
        this.Marca = marca;

    }

    public void setModelo(String modelo) {
        this.Modelo = modelo;

    }

    public void setMarca(String marca) {
        this.Marca = marca;

    }

    public String getModelo() {
        return Modelo;

    }

    public String getMarca() {
        return Marca;

    }

    public String output(){
        return Modelo.toLowerCase() + " " + Marca.toLowerCase();

    }

    public String output(boolean showUltimoNome){

        if(showMarca){
            return output();

        }
        return Marca;

    }

}
